server {
    listen 80;
    server_name localhost;

    root /usr/share/nginx/html;
    index index.html;

    location = /page1.html {
        return 301 /page-2.html;
    }

    location / {
        add_header X-App-Env "$APP_ENV";
        try_files $uri $uri/ =404;
    }
} 